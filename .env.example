# Supabase 数据库配置
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_OR_ANON_KEY=your-public-or-anon-key

# 轮询间隔(秒)
CHECK_POLL_INTERVAL_SECONDS=60

# ==========================================
# 注意:CHECK 配置已迁移到数据库
# ==========================================
#
# 配置不再通过环境变量管理,请使用以下 SQL 命令在 Supabase 中添加配置:
#
# INSERT INTO check_configs (name, type, model, endpoint, api_key, enabled)
# VALUES (
#   '主力 OpenAI',                                     -- 显示名称
#   'openai',                                         -- 提供商类型: openai/gemini/anthropic
#   'gpt-4o-mini',                                    -- 模型名称
#   'https://api.openai.com/v1/chat/completions',    -- API 端点
#   'sk-your-openai-key',                            -- API 密钥
#   true                                              -- 是否启用
# );
#
# 注: id 字段会自动生成 UUID,无需手动指定
#
# 更多配置示例:
